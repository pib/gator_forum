{
  "name": "vanilla-bootstrap",
  "private": true,
  "main": [
    "less/vanilla-bootstrap.less",
    "js/src/main.js"
  ],
  "ignore": [
    "about.php",
    "screenshot.png",
    "package.json",
    "gulpfile.js",
    "views",
    "design",
    "js/custom.js",
    "less/style.less",
    "less/custom.less",
    "less/themes"
  ],
  "dependencies": {
    "bootstrap": "3.2.0",
    "bootstrap-social": "4.5.1",
    "bootswatch": "3.2.x",
    "font-awesome": "4.1.0",
    "spin.js": "2.0.1",
    "jquery-icheck": "2.x-beta"
  },
  "overrides": {
    "bootstrap-social": {
      "main": "bootstrap-social.less"
    },
    "font-awesome": {
      "main": "less/font-awesome.less"
    },
    "jquery": {
      "main": []
    },
    "jquery-icheck": {
      "main": "icheck.js"
    },
    "spin.js": {
      "main": [
        "spin.js",
        "jquery.spin.js"
      ]
    }
  }
}
